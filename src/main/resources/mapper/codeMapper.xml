<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.park.public_gs.mapper.CodeMapper">	<!-- mapper 인터페이스가 위치하는 경로 -->

    <select id="codeMasterList" resultType="org.park.public_gs.vo.CodeMasterVo">
        SELECT seq, commonCode, commonName, remark
        FROM code_master
    </select>

    <select id="codeDetailList" parameterType="String" resultType="org.park.public_gs.vo.CodeDetailVo">
        SELECT code, codeName, sortNum, remark, Del_Tag
        FROM code_detail
        WHERE commonCode = #{commonCode}
    </select>

    <!--<select id="CommonCodeSpaceInfo" resultType="org.park.public_gs.vo.CommonCodeVo">	&lt;!&ndash; id: mapper 인터페이스에서 선언한 메서드 이름과 같아야 한다. resultType: VO 클래스가 위치하는 경로 &ndash;&gt;
        SELECT spaceNo as codeNo, spaceNm as codeNm
        FROM space_Info	&lt;!&ndash; SQL문 작성 &ndash;&gt;
    </select>

    <select id="CommonCodeUserList" resultType="org.park.public_gs.vo.CommonCodeVo">	&lt;!&ndash; id: mapper 인터페이스에서 선언한 메서드 이름과 같아야 한다. resultType: VO 클래스가 위치하는 경로 &ndash;&gt;
        SELECT seq as codeNo, userNm as codeNm
        FROM user_Info
        where del_Tag='0'&lt;!&ndash; SQL문 작성 &ndash;&gt;
    </select>

    <select id="CommonCodeDiscList" resultType="org.park.public_gs.vo.CommonCodeVo">	&lt;!&ndash; id: mapper 인터페이스에서 선언한 메서드 이름과 같아야 한다. resultType: VO 클래스가 위치하는 경로 &ndash;&gt;
        SELECT discCode as codeNo, discNm as codeNm
        FROM discount_Info	&lt;!&ndash; SQL문 작성 &ndash;&gt;
    </select>-->
</mapper>
